one_long_list = [30, 94, 34, 27, 65, 26, 59, 9, 32, 72, 30, 66, 97, 51, 21, 48, 63, 85, 48, 55, 35, 82, 68, 80, 72, 28, 67, 15, 92, 70, 94, 40, 93, 99, 17, 8, 29, 51, 100, 11, 42, 92, 24, 35, 69, 64, 41, 98, 51, 46, 48, 68, 92, 57, 67, 23, 31, 54, 41, 38, 81, 93, 93, 91, 20, 96, 59, 16, 60, 61, 20, 29, 64, 98, 68, 4, 85, 64, 66, 0, 81, 68, 86, 43, 33, 3, 44, 67, 9, 15, 5, 57, 27, 53, 7, 91, 59, 10, 7]

print(max(one_long_list))
print(min(one_long_list))
print(one_long_list.index(max(one_long_list)))
largest = 30

for x in one_long_list:
    if x > largest:
        largest = x

print(largest)
